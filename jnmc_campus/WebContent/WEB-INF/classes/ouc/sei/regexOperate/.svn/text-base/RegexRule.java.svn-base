package ouc.sei.regexOperate;

public class RegexRule {

	private String urlRegex = null;   //url正则表达式匹配规则
	private String regexTem = null;     //规则对应的xml模板
	
	/**
	 * default constructor
	 */
	public RegexRule(){
		
	}
	

	/**
	 * 含餐构造函数
	 * @param urlRegex  正则表达式匹配规则
	 * @param xmlTem    xml模板
	 */
	public RegexRule(String urlRegex, String regexTem){
		this.urlRegex = urlRegex;
		this.regexTem = regexTem;
		
	}


	public String getUrlRegex() {
		return urlRegex;
	}


	public void setUrlRegex(String urlRegex) {
		this.urlRegex = urlRegex;
	}


	public String getRegexTem() {
		return regexTem;
	}


	public void setRegexTem(String regexTem) {
		this.regexTem = regexTem;
	}
	
	
	
}
